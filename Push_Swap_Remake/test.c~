#include "push_swap.h"

int main()
{
	t_sol *sol;
	t_sol *autre_sol;

	sol = NULL;
	autre_sol = NULL;
	ft_add_sol(&sol, "ra");
	ft_add_sol(&sol, "ra");
	ft_add_sol(&sol, "pa");
	ft_add_sol(&sol, "sa");
	ft_add_sol(&sol, "rra");
	ft_add_sol(&sol, "rra");
	ft_add_sol(&autre_sol, "rra");
	ft_add_sol(&autre_sol, "pa");
	ft_add_sol(&autre_sol, "pb");
	ft_add_sol(&autre_sol, "pb");
	ft_add_num_sol(&sol, autre_sol);

	char opts[5] = {'\0', '\0', '\0', '\0', '\0'};
	ft_display(sol, opts);
  	return(0);	
}
